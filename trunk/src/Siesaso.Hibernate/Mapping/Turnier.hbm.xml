<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="Softwarekueche.Siesaso.Hibernate"
                   assembly="Siesaso.Hibernate">

    <class name="Turnier">

        <id name="Id" type="Int32">
            <generator class="identity" />
        </id>

        <property name="Name" type="String" />

        <property name="Beschreibung" type="String" />

        <property name="Datum" type="DateTime" />

        <property name="DatumReal" type="DateTime" />

        <property name="Ausrichter" type="String" />

        <property name="Leitung" type="String" />

        <property name="Listenleitung" type="String" />

    </class>
    
    
</hibernate-mapping>
                   